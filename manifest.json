{
  "manifest_version": 3,
  "name": "Kibana as Code",
  "version": "1.0.0",
  "description": "Export Kibana dashboards, visualizations, and other resources as code",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "sidePanel",
    "storage"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "side_panel": {
    "default_path": "sidepanel/sidepanel.html"
  }
}
